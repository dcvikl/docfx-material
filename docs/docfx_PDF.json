{
  "pdf": {
    "content": [
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md",
          "api/**.md",
          "api/**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appFooter": "<span>Copyright © 2020 Cvikl Dubravko<br>Based on: DocFx Material theme by Oscar Vasquez<br>Generated with <strong>DocFX</strong></span>",
      "_appTitle": "DocFX Material",
      "_projectTitle": "Gastro v2 SQL - Korisnički priručnik",
      "_enableSearch": "true",
      "_gitContribute": {
        "repo": "https://github.com/dcvikl/docfx-material"
      },
      "_gitUrlPattern": "github"
    },
    "markdownEngineName": "markdig",
    "dest": "_site/PDF",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "tocTitle": "Sadržaj",
    "wkhtmltopdf": {
      "additionalArguments": "--debug-javascript --enable-local-file-access --enable-toc-back-links --header-html ../pdf.hr/header.html --footer-html ../pdf.hr/footer.html --print-media-type --margin-left 25mm"
    },
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false,
    "template": [
      "pdf.default",
      "../pdf.hr"
    ]
  }
}